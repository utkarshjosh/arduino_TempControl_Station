void setTemp(){
//Mode travelst through and set increments furhter mode saves 
display.clearDisplay();
int low=temp_min;
int high =temp_max;
if(MODE_press==1 && SET_exchange ){
low++;
if(low>26)
low=18;
SET_exchange=false;
temp_min=low;
 EEPROM.update(0, temp_min); 
}

else if(MODE_press==2 && SET_exchange){
high++;
if(high>38)
high=30;
SET_exchange=false;
temp_max=high;
 EEPROM.update(1, temp_max);
 
}

//if(MODE_exchange)
//{temp_min=low;
//temp_max=high;
//MODE_exchange=false;
//}
 display.setTextSize(2);
 display.setTextColor(WHITE);
 display.setCursor(12,3 );
 display.print("Set Range");
 display.setCursor(28,25);
 display.print(low);
  display.setCursor(58,25);
 display.print("-");
 display.setCursor(76,25);
 display.print(high);
 int rectX=25;
 if(MODE_press==2)
  rectX=74;
  display.fillRect(rectX,44, 28,4,WHITE);
 
 display.display();
 
}

void switchFan(bool i){
  digitalWrite(5,i); // Fan relay pin 5
  fan_state=i;
 }
 void switchLamp(bool i){
 digitalWrite(6,i); //Lamp relay pin 6
 lamp_state=i;
 }
 
